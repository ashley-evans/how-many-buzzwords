{
    "name": "buzzword-aws",
    "version": "1.0.0",
    "devDependencies": {
        "@apify/consts": "^1.3.0",
        "@tsconfig/node14": "^1.0.1",
        "@types/fs-extra": "^9.0.13",
        "@types/jest": "^27.0.3",
        "@types/node": "^16.11.12",
        "@typescript-eslint/eslint-plugin": "^5.6.0",
        "@typescript-eslint/parser": "^5.6.0",
        "aws-sdk-client-mock": "^0.5.6",
        "copyfiles": "^2.4.1",
        "eslint": "^7.32.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jest": "^24.4.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.1.0",
        "fs-extra": "^10.0.0",
        "husky": "^7.0.4",
        "jest": "^27.4.3",
        "jest-mock-extended": "^2.0.4",
        "lerna": "^4.0.0",
        "nock": "^13.1.3",
        "npm-package-json-lint": "^5.4.2",
        "ts-jest": "^27.1.1",
        "typescript": "^4.5.2",
        "wscat": "^5.0.0"
    },
    "dependencies": {
        "@aws-sdk/client-dynamodb": "3.34.0",
        "@middy/core": "2.5.1",
        "@middy/http-error-handler": "2.5.1",
        "@middy/sqs-json-body-parser": "2.5.1",
        "@middy/validator": "2.5.1",
        "http-status-codes": "2.1.4"
    },
    "scripts": {
        "lint-js": "node node_modules/.bin/eslint '.' --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
        "lint-packages": "node node_modules/.bin/npmPkgJsonLint '.'",
        "lint": "npm run lint-js && npm run lint-packages",
        "i": "npm i && lerna bootstrap",
        "ci": "npm ci && lerna bootstrap --ci",
        "clean": "rm -rf node_modules && lerna clean -y",
        "test": "node node_modules/.bin/jest --verbose",
        "prepare": "husky install",
        "precompile": "npm run ci",
        "compile": "tsc --project ./tsconfig.build.json",
        "postcompile": "./node_modules/.bin/copyfiles -E -u 1 applications/**/*-template.yml applications/**/package*.json applications/**/Makefile dist"
    }
}
